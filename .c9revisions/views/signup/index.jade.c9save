{"ts":1361749968594,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"extends ../../layouts/default\n\nblock head\n  title Sign Up\n\nblock feet\n  script(src='/views/signup/index.js')\n\nblock body\n  div.page-header\n    h1 Sign Up\n  .row\n    .span5\n      #signup\n  .row\n    .span5\n      a.btn.btn-large.btn-block(href=\"/auth/twitter\") <i class=\"icon-twitter icon-large\"></i> Connect with Twitter\n      a.btn.btn-large.btn-block(href=\"/auth/github\") <i class=\"icon-github icon-large\"></i> Connect with GitHub\n      \n  script(type='text/template', id='tmpl-signup')\n    form.form-horizontal\n      div.alerts\n        |<% _.each(errors, function(err) { %>\n        div.alert.alert-error\n          |<%= err %>\n        |<% }); %>\n      div.control-group(class!='<%= errfor.username ? \"error\" : \"\" %>')\n        label.control-label Pick a Username:\n        div.controls\n          input(type='text', name='username', value!='<%= username %>')\n          span.help-inline <%= errfor.username %>\n      div.control-group(class!='<%= errfor.email ? \"error\" : \"\" %>')\n        label.control-label Enter Your Email:\n        div.controls\n          input(type='text', name='email', value!='<%= email %>')\n          span.help-inline <%= errfor.email %>\n      div.control-group(class!='<%= errfor.password ? \"error\" : \"\" %>')\n        label.control-label Create a Password:\n        div.controls\n          input(type='password', name='password', value!='<%= password %>')\n          span.help-inline <%= errfor.password %>\n      div.form-actions\n        button.btn.btn-primary.btn-signup(type='button') Create My Account\n"]],"start1":0,"start2":0,"length1":0,"length2":1519}]],"length":1519}
